{
  "name": "@fundset/indexer",
  "version": "0.1.0",
  "scripts": {
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "mocha": "ts-mocha test/**/*.ts",
    "codegen": "envio codegen",
    "predev": "pnpx tsx generate-config.ts",
    "dev": "nodemon --watch . --ignore node_modules --ignore generated/node_modules --ext ts,tsx,js,json,graphql,yaml,yml,env,sol --signal SIGTERM --exec \"pnpm run dev:once\"",
    "dev:once": "envio stop && envio dev",
    "stop": "envio stop",
    "start": "envio start",
    "test": "pnpm mocha",
    "postinstall": "pnpx tsx postinstall.ts"
  },
  "devDependencies": {
    "@catalystic/json-to-yaml": "^1.0.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "ts-mocha": "^10.0.0",
    "typescript": "5.2.2",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "envio": "2.29.1"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

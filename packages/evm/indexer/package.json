{
  "name": "@fundset/indexer",
  "version": "0.1.0",
  "scripts": {
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "mocha": "ts-mocha test/**/*.ts",
    "codegen": "envio codegen",
    "predev": "pnpx tsx generate-config.ts",
    "dev": "nodemon --watch . --ignore node_modules --ignore generated/node_modules --ext ts,tsx,js,json,graphql,env,sol --signal SIGTERM --exec \"pnpm run dev:once\"",
    "dev:once": "envio stop && envio dev",
    "stop": "envio stop",
    "start": "envio start",
    "test": "pnpm mocha",
    "postinstall": "pnpx tsx postinstall.ts"
  },
  "devDependencies": {
    "@catalystic/json-to-yaml": "^1.0.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "chai": "4.3.10",
    "dotenv": "^17.2.3",
    "mocha": "10.2.0",
    "nodemon": "^3.1.0",
    "ts-mocha": "^10.0.0",
    "typescript": "5.2.2",
    "ts-node": "10.9.1",
    "@rescript/react": "0.12.1",
    "bignumber.js": "9.1.2",
    "date-fns": "3.3.1",
    "ethers": "6.8.0",
    "express": "4.20.0",
    "ink": "3.2.0",
    "ink-big-text": "1.2.0",
    "ink-spinner": "4.0.3",
    "js-sdsl": "4.4.2",
    "pino": "8.16.1",
    "postgres": "3.4.1",
    "react": "18.2.0",
    "rescript": "11.1.3",
    "rescript-envsafe": "5.0.0",
    "rescript-schema": "9.3.0",
    "envio": "2.30.1",
    "viem": "2.21.0",
    "yargs": "17.7.2",
    "prom-client": "15.0.0"
  },
  "dependencies": {
    "envio": "2.30.1"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
